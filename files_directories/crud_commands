# Create Files
touch name = create file
touch path/name = can create file in path

# Create Directories
mkdir = make directory
mkdir directory_name

# Copy files
cp [source] [destination]
cp file directory/ = copy to directoy with same name
cp file directory/new_name = copy to directory with new name 

# Copy directory
cp -r [source] [dest] 
cp -r directory/ BackUpDirectory/

# Moving files
mv [source] [dest]
mv file directory/ = move file to the directory
mv file_name new_file_name = rename a file
mv directory/ new_directory/ = rename a directory

# Delete files
rm = remove
rm file = delete a file
rm -r directory/ = delete a directory
